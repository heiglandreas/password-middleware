	"keywords": [
		"password",
		"middleware",
		"psr"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Heigl",
			"email": "andreas@heigl.org"
		}
	],
	"require": {
		"php": "^7.3|^8.0",
		"psr/http-message": "^1.0",
		"psr/http-server-middleware": "^1.0",
		"org_heigl/password": "^1.0.0"
	},
	"require-dev": {
		"laminas/laminas-diactoros": "^2.5",
		"phpunit/phpunit": "^8.0|^9.0"
	},
	"autoload": {
		"psr-4": {
			"Org_Heigl\\PasswordMiddleware\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Org_Heigl\\PasswordMiddlewareTest\\": "tests/"
		}
